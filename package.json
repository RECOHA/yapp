{
  "name": "yapp-api",
  "version": "1.0.0",
  "description": "API simples para manipulação de vídeo e download do YouTube",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.js",
    "build": "tsc",
    "preinstall": "node -e \"if(process.env.YOUTUBE_DL_SKIP_PYTHON_CHECK!=='1'){process.env.YOUTUBE_DL_SKIP_PYTHON_CHECK='1'}\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "youtube-dl-exec": "^2.4.9"
  },
  "overrides": {
    "youtube-dl-exec": {
      "bin-version-check": "5.0.0"
    }
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node": "^20.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
